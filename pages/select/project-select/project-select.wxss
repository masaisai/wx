/* pages/select/project-select/project-select.wxss */
page{height:100%}
.container{padding:10rpx 0;font-size:26rpx;padding-top:100rpx;position:relative;height:100%;overflow:auto}
.searchBar{width:750rpx;height:100rpx;position:fixed;left:0;top:0;z-index:2;background:white;border-bottom:1rpx solid #eee}
.projectWrap{width:100%;}
.projectWrap .item{width:100%;border-bottom:1rpx solid #eee;line-height:60rpx;position:relative;background: white;font-size:26rpx;box-sizing:border-box}
.projectWrap .item .con{font-size:26rpx;text-align:left;padding:20rpx;}
.projectWrap .item .check{color:#11c260;position:absolute;right:20rpx;top:20rpx;font-size:30rpx;display:none}
.projectWrap .item.active .check{display:block}
.loadData{line-height:60rpx;text-align:center;font-size:24rpx}
.noData{line-height:60rpx;text-align:center;font-size:24rpx}

.container .search{
  width: 725rpx;
  height: 70rpx;
  padding:15rpx 12.5rpx;
  background:#fff;
  display:flex
}
.container .search-left{
  flex: 8;
  background: #fff;
  text-align: left;
  display:flex
}
.container .search-left input{
  display: inline-block;
  height: 70rpx;
  font-size: 26rpx;
  flex:10;
  padding:0 20rpx;
  color:#666;
  background-color:#f2f2f2;
}
.search-placeholder{
  color:#aaa;
  line-height: 20rpx;
  z-index:0
}
.container .search .search-left button{
  display: inline-block;
  flex:1;
 
}
.container .search .search-right{
  flex: 1;
}
button.searchBtn{ background-color:#f2f2f2!important;border:0px solid transparent;font-size:32rpx!important;outline:none!important;color:#666;line-height:70rpx;border-radius:0}
button.searchBtn .iconfont,button.addBtn .iconfont{font-size:20px!important}